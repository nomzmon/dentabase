                </div>

                <div class="line-6"></div>
                <div class="line-7"></div>
                <div class="frame-2">
                    <img class="iconixto-linear" src="/img/icon-edit.svg" />
                    <div class="text-wrapper-19">Edit</div>
                </div>
                <div class="frame-3">
                    <img class="iconixto-solid-tick" src="/img/icon-check.svg" />
                    <div class="text-wrapper-20">Done</div>
                </div>
            </div>
        </div>


        <!-- `PATIENT DATA-->
        <div class="frame-4">
            <div class="group-4">
                <div class="group-wrapper">
                    <div class="group-5">
                        <div class="text-wrapper-21">PATIENT DATA</div>
                        <div class="line-8"></div>
                    </div>
                </div>
                <div class="patient-info-group">
                    <div class="birthdate-group">
                        <div class="text-wrapper-22">Birthdate (mm/dd/yy)</div>
                        <div class="text-wrapper-23">{{birth_month}} {{birth_date}}, {{birth_year}}</div>
                    </div>
                    <div class="age-group">
                        <div class="text-wrapper-24">Age</div>
                        <div class="text-wrapper-25">{{age}}</div>
                    </div>
                    <div class="address-group">
                        <div class="text-wrapper-26">Home Address</div>
                        <p class="text-wrapper-27">{{home_address}}</p>
                    </div>
                    <div class="occupation-group">
                        <div class="text-wrapper-24">Occupation</div>
                        <div class="text-wrapper-28">{{personal_occupation}}</div>
                    </div>
                    <div class="religion-group">
                        <div class="text-wrapper-29">Religion</div>
                        <div class="text-wrapper-30">{{religion}}</div>
                    </div>
                    <div class="nationality-group">
                        <div class="text-wrapper-31">Nationality</div>
                        <div class="text-wrapper-32">{{nationality}}</div>
                    </div>
                    <div class="dental-insurance">
                        <div class="text-wrapper-33">Dental Insurance</div>
                        <div class="text-wrapper-34">{{dental_insurance}}</div>
                    </div>
                    <div class="nickname-group">
                        <div class="text-wrapper-35">Nickname</div>
                        <div class="text-wrapper-36">{{nickname}}</div>
                    </div>
                    <div class="sex-group">
                        <div class="text-wrapper-35">Sex (M/F)</div>
                        <div class="text-wrapper-36">{{sex}}</div>
                    </div>

                    <!--`END-->


                    <!--`CONTACT INFORMATION-->
                    <div class="contact-info-group">
                        <div class="group-6">
                            <div class="fax-num-group">
                                <div class="text-wrapper-38">Fax No.</div>
                                <div class="text-wrapper-37">{{fax_number}}</div>
                            </div>
                            <div class="office-num-group">
                                <div class="text-wrapper-40">Office No.</div>
                                <div class="text-wrapper-39">{{office_number}}</div>
                            </div>
                            <div class="phone-num-group">
                                <div class="text-wrapper-42">Cel/Mobile No.</div>
                                <div class="text-wrapper-41">{{mobile_number}}</div>
                            </div>
                            <div class="home-num-group">
                                <div class="text-wrapper-43">Home No.</div>
                                <div class="text-wrapper-37">{{home_number}}</div>
                            </div>
                            <div class="email-address-group">
                                <div class="text-wrapper-38">Email Address</div>
                                <div class="text-wrapper-44">{{email_address}}</div>

                            </div>
                        </div>
                        <div class="group-7">
                            <div class="text-wrapper-45">CONTACT INFORMATION</div>
                            <div class="line-8"></div>
                        </div>
                        <!--`END-->


                        <!--`DENTAL HISTORY-->
                    </div>
                    <div class="dental-history-group">
                        <div class="group-8">
                            <div class="text-wrapper-45">DENTAL HISTORY</div>
                            <div class="line-8"></div>
                        </div>
                        <div class="previous-dentist">
                            <div class="text-wrapper-24">Previous Dentist</div>
                            <div class="text-wrapper-46">{{previous-dentist}}</div>
                        </div>
                        <div class="last-visit-group">
                            <div class="text-wrapper-24">Last Dental Visit</div>
                            <div class="text-wrapper-47">{{LDV_month}} {{LDV_date}}, {{LDV_year}}</div>
                        </div>
                    </div>
                    <!--`END-->


                    <!--`FOR MINORS-->
                    <div class="minors-group">
                        <div class="parent-name-group">
                            <div class="text-wrapper-54">Parent/Guardianâ€™s Name</div>
                            <div class="text-wrapper-55">{{guardian_name}}</div>
                        </div>
                        <div class="parent-occupatin">
                            <div class="text-wrapper-52">Occupation</div>
                            <div class="text-wrapper-53">{{guardian_occupation}}</div>
                        </div>
                        <div class="refer-you-group">
                            <p class="text-wrapper-50">Whom may we thank for refering you?</p>
                            <div class="text-wrapper-51">{{minor_question_one}}</div>
                        </div>
                        <div class="reason-consulation">
                            <p class="text-wrapper-48">What is your reason for dental consulation</p>
                            <div class="text-wrapper-49">{{minor_question_two}}</div>
                        </div>



                        <div class="group-9">
                            <div class="text-wrapper-56">FOR MINORS</div>
                            <div class="line-8"></div>
                        </div>
                    </div>
                    <!--`END-->
                </div>
            </div>


            <!-- `TREATMENT RECORD -->
            <div class="treatment-record">
                <div class="overlap-2">
                    <div class="div-wrapper">
                        <div class="group-10">
                            <div class="group-11">
                                <div class="group-12">
                                    <img class="icon-7" src="/img/icon-updown.svg" />
                                    <div class="text-wrapper-57">Tooth No./s</div>
                                </div>
                            </div>
                            <div class="group-13">
                                <img class="icon-8" src="/img/icon-updown.svg" />
                                <div class="text-wrapper-58">Procedure</div>
                            </div>
                            <div class="group-14">
                                <div class="overlap-group-2">
                                    <img class="icon-9" src="/img/icon-updown.svg" />
                                    <div class="text-wrapper-59">Dentist</div>
                                </div>
                            </div>
                            <div class="group-15">
                                <img class="icon-10" src="/img/icon-updown.svg" />
                                <div class="text-wrapper-60">Amount Charged</div>
                            </div>
                            <div class="group-16">
                                <div class="overlap-3">
                                    <img class="icon-11" src="/img/icon-updown.svg" />
                                    <div class="text-wrapper-60">Amount <br />Paid</div>
                                </div>
                            </div>
                            <div class="overlap-4">
                                <img class="icon-12" src="/img/icon-updown.svg" />
                                <div class="text-wrapper-61">Next Appointment</div>
                            </div>
                            <div class="group-17">
                                <div class="overlap-5">
                                    <div class="text-wrapper-61">Balance</div>
                                    <img class="icon-13" src="/img/icon-updown.svg" />
                                </div>
                            </div>
                            <div class="group-18">
                                <img class="icon-14" src="/img/icon-updown.png" />
                                <div class="text-wrapper-62">Date</div>
                            </div>
                        </div>
                    </div>
                    <div class="list-treatment">
                        <div class="overlap-6">
                            <div class="treatment-record-2">
                                <div class="line-9"></div>
                                <div class="line-10"></div>
                                <div class="line-11"></div>
                                <div class="line-12"></div>
                                <div class="line-13"></div>
                                <div class="line-14"></div>
                                <div class="line-15"></div>
                                <div class="line-16"></div>
                            </div>

                            <div class="patient-info">
                                <div class="text-wrapper-63">{{current_dentist}}</div>
                                <div class="text-wrapper-64">{{amount_charged}}</div>
                                <div class="text-wrapper-65">{{amount_paid}}</div>
                                <div class="overlap-7">
                                    <div class="text-wrapper-66">{{balance}}</div>
                                    <div class="text-wrapper-67">{{next_appointment_date}}</div>
                                </div>
                                <div class="overlap-8">
                                    <div class="text-wrapper-68">{{treatment_date}}</div>
                                    <div class="group-19">
                                        <div class="element-wrapper">
                                            <div class="element">{{tooth_number}}<br />60</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="group-20">
                                    <div class="overlap-9">
                                        <div class="rectangle-3"></div>
                                        <div class="tooth-cleaning">{{treatment_procedures}}, sdfdsf <br />restortion,
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="patient-info-2">
                                <div class="text-wrapper-63">Dr.&nbsp;&nbsp;Doroteo</div>
                                <div class="text-wrapper-64">1,000,000</div>
                                <div class="text-wrapper-65">1,000,000</div>
                                <div class="overlap-7">
                                    <div class="text-wrapper-66">1,000,000</div>
                                    <div class="text-wrapper-67">December 30, 2024</div>
                                </div>
                                <div class="overlap-8">
                                    <div class="text-wrapper-68">February 20, 2030</div>
                                    <div class="group-19">
                                        <div class="element-wrapper">
                                            <div class="element">53<br />60</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="group-20">
                                    <div class="overlap-9">
                                        <div class="rectangle-3"></div>
                                        <div class="tooth-cleaning">tooth cleaning, sdfdsf <br />restortion,</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="group-21">
                    <div class="group-22">
                        <div class="text-wrapper-69">Treatment Record</div> <img class="line-8"
                            src="/img/line-6-5.svg" />
                    </div>
                </div>
                <div class="group-23">
                    <div class="group-24">
                        <div class="overlap-group-3">
                            <div class="text-wrapper-70">1000 of 1000</div> <img class="vector-2"
                                src="/img/vector-2.svg" />
                        </div>
                    </div>
                </div>
            </div>


            <!--`MEDICAL HISTORY-->
            <div class="medical-history">
                <div class="group-25">
                    <div class="text-wrapper-71">MEDICAL HISTORY</div> <img class="line-8" src="/img/line-6-3.svg" />
                </div>
                <div class="medical-history-2">
                    <div class="group-26">
                        <div class="text-wrapper-72">Name of Physician</div>
                        <div class="text-wrapper-73">{{physician_name}}</div>
                    </div>
                    <div class="group-27">
                        <div class="text-wrapper-72">Specialty, If applicable</div>
                        <div class="text-wrapper-74">{{physician_specialty}}</div>
                    </div>
                    <div class="group-28">
                        <div class="group-29">
                            <div class="text-wrapper-72">Office Number</div>
                            <div class="text-wrapper-75">{{physician_office_number}}</div>
                        </div>
                    </div>
                    <div class="group-30">
                        <div class="text-wrapper-76">Office Address</div>
                        <p class="text-wrapper-77">{{physician_address}}</p>
                    </div>
                    <div class="group-31">
                        <p class="text-wrapper-78">Do you use tobacco products?</p>
                        <div class="group-32">

                            <!-- Yes option -->
                            <div class="text-wrapper-79">Yes</div>
                            <label>
                                <input type="radio" id="radio-yes" name="tobacco-question" value="yes"
                                    {{#if is_tobacco_user}}checked{{/if}}>
                                <div class="rectangle-4"></div>
                            </label>

                            <!-- No option -->
                            <div class="text-wrapper-80">No</div>
                            <label>
                                <input type="radio" id="radio-no" name="tobacco-question" value="no"
                                    {{#if is_not_tobacco_user}}checked{{/if}}>
                                <div class="rectangle-5"></div>
                            </label>
                        </div>
                    </div>

                    <div class="group-33">
                        <div class="group-34">

                            <!-- Yes option -->
                            <div class="text-wrapper-79">Yes</div>
                            <label>
                                <input type="radio" id="radio-yes" name="alcohol-question" value="yes"
                                    {{#if is_alcohol_user}}checked{{/if}}>
                                <div class="rectangle-4"></div>
                            </label>

                            <!-- No option -->
                            <div class="text-wrapper-80">No</div>
                            <label>
                                <input type="radio" id="radio-no" name="alcohol-question" value="no"
                                    {{#if is_not_alcohol_user}}checked{{/if}}>
                                <div class="rectangle-5"></div>
                            </label>
                        </div>

                        <div class="group-35">
                            <p class="text-wrapper-81">Do you use alcohol, cocaine or other dangerous drugs?</p>
                        </div>


                    </div>
                    <div class="group-36">


                        <div class="group-35">
                            <p class="text-wrapper-81">Are you allergic to any of the following:</p>
                            <div class="group-37">
                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="allergic-question" value="yes"
                                        {{#if is_allergic_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="allergic-question" value="no"
                                        {{#if is_not_allergic_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>
                            </div>
                        </div>

                        <!-- Allergy options with checkboxes -->
                        <div class="group-38">
                            <div class="text-wrapper-79">Local Anesthetic (ex. Lidocaine)</div>
                            <label>
                                <input type="checkbox" id="checkbox-lidocaine" name="allergy" value="lidocaine">
                                <div class="rectangle-4"></div>
                            </label>
                        </div>
                        <div class="group-39">
                            <div class="text-wrapper-79">Penicillin, Antibiotics</div>
                            <label>
                                <input type="checkbox" id="checkbox-penicillin" name="allergy" value="penicillin">
                                <div class="rectangle-4"></div>
                            </label>
                        </div>
                        <div class="group-40">
                            <div class="text-wrapper-79">Sulfa Drugs</div>
                            <label>
                                <input type="checkbox" id="checkbox-sulfa" name="allergy" value="sulfa">
                                <div class="rectangle-4"></div>
                            </label>
                        </div>
                        <div class="group-41">
                            <div class="text-wrapper-79">Aspirin</div>
                            <label>
                                <input type="checkbox" id="checkbox-aspirin" name="allergy" value="aspirin">
                                <div class="rectangle-4"></div>
                            </label>
                        </div>
                        <div class="group-42">
                            <div class="text-wrapper-79">Latex</div>
                            <label>
                                <input type="checkbox" id="checkbox-latex" name="allergy" value="latex">
                                <div class="rectangle-4"></div>
                            </label>
                        </div>

                        <!-- Others allergy checkbox and input field -->
                        <div class="group-43">
                            <div class="text-wrapper-79">Others:</div>
                            <label>
                                <input type="checkbox" id="checkbox-others">
                                <div class="rectangle-6"></div>
                            </label>
                            <!-- Text input field for specifying other allergies -->
                            <input type="text" id="other-allergy-input" placeholder="Please specify" class="hidden">
                        </div>



                    </div>
                    <div class="group-44">

                        <div class="overlap-11">
                            <p class="are-you-under"> Are you under medical treatment
                                now?<br />&nbsp;&nbsp;&nbsp;If
                                yes, what is the condition being treated: </p>
                            <div class="group-45">

                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="med-treatment-question" value="yes"
                                        {{#if is_med_treatment_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="med-treatment-question" value="no"
                                        {{#if is_not_med_treatment_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>

                            </div>
                        </div>
                        <div class="rectangle-7"></div>

                    </div>
                    <div class="group-46">
                        <div class="overlap-12">
                            <div class="overlap-13">
                                <p class="have-you-ever-had"> Have you ever had serious illness or surgical
                                    operation?<br />&nbsp;&nbsp;&nbsp;If
                                    yes, what illness or operation: </p>
                                <div class="rectangle-8"></div>
                            </div>
                            <div class="group-47">

                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="illness-question" value="yes"
                                        {{#if is_illness_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="illness-question" value="no"
                                        {{#if is_not_illness_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>
                            </div>

                        </div>
                    </div>
                    <div class="group-48">
                        <div class="overlap-14">
                            <div class="overlap-15">
                                <p class="are-you-taking-any">
                                    Are you taking any prescription/non-prescription
                                    medication?<br />&nbsp;&nbsp;&nbsp;If
                                    yes, please specify:
                                </p>
                                <div class="rectangle-9"></div>
                            </div>
                            <div class="group-49">

                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="prescription-question" value="yes"
                                        {{#if is_prescription_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="prescription-question" value="no"
                                        {{#if is_not_prescription_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>

                            </div>
                        </div>
                    </div>
                    <div class="group-50">
                        <div class="overlap-16">
                            <div class="for-women-only-are-wrapper">
                                <p class="for-women-only-are">
                                    For Women Only:&nbsp;&nbsp;&nbsp;&nbsp;
                                    Are you
                                    pregnant?<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                    Are you
                                    nursing?<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                    Are you taking birth control pills?
                                </p>
                            </div>
                            <div class="group-51">

                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="pregnant-question" value="yes"
                                        {{#if is_pregnant_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="pregnant-question" value="no"
                                        {{#if is_not_pregnant_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>
                            </div>

                            <div class="group-52">

                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="nursing-question" value="yes"
                                        {{#if is_nursing_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="nursing-question" value="no"
                                        {{#if is_not_nursing_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>
                            </div>

                            <div class="group-53">
                                <div class="text-wrapper-79">Yes</div>
                                <label>
                                    <input type="radio" id="radio-yes" name="birth_control-question" value="yes"
                                        {{#if is_birth_control_user}}checked{{/if}}>
                                    <div class="rectangle-4"></div>
                                </label>
                                <div class="text-wrapper-80">No</div>
                                <label>
                                    <input type="radio" id="radio-no" name="birth_control-question" value="no"
                                        {{#if is_not_birth_control_user}}checked{{/if}}>
                                    <div class="rectangle-5"></div>
                                </label>

                            </div>
                        </div>
                    </div>


                    <div class="overlap-17">
                        <div class="group-54">
                            <p class="text-wrapper-82">Do you have or have you had any of the following? Check that
                                apply</p>
                            <div class="medical-condition">
                                <div class="group-55">                                  


                                    <div class="text-wrapper-83">Stroke</div>
                                     <label>
                                        <input type="checkbox" id="checkbox-stroke" name="medical-condition" value="stroke">
                                        <div class="rectangle-4"></div>
                                    </label>
                                </div>
                               
                                <div class="group-56">
                                    <div class="text-wrapper-83">High Blood Pressure</div>
                                    <label>
                                        <input type="checkbox" id="checkbox-high-blood" name="medical-condition" value="high-blood">
                                        <div class="rectangle-4"></div>
                                    </label>
                                </div>
                                <div class="group-57">
                                    <div class="text-wrapper-83">Heart Disease</div>
                                     <label>
                                        <input type="checkbox" id="checkbox-heart-disease" name="medical-condition" value="heart-disease">
                                        <div class="rectangle-4"></div>
                                    </label>
                                </div>
                                <div class="group-58">
                                    <div class="text-wrapper-83">Cancer / Tumors</div>
                                  <label>
                                        <input type="checkbox" id="checkbox-cancer" name="medical-condition" value="cancer">
                                        <div class="rectangle-4"></div>
                                    </label>
                                </div>
                                <div class="group-59">
