</head>

<body>



    <!-- + MAIN INTERFACE Patient's Details-->
    <div class="patient-information">
        <div class="text-wrapper-3">Patients</div>
        <div class="overlap-wrapper">
            <div class="text-wrapper-10">Patient Information</div>
            <div class="text-wrapper-11">Medical History</div>
            <div class="line"></div>
            <p class="text-wrapper-12">{{full_name}} | {{age}} | {{sex}}</p>
            <div class="line-2"></div>
            <div class="text-wrapper-13">Dental Record Chart</div>
            <div class="text-wrapper-14">Informed Consent</div>
            <div class="text-wrapper-15">Treatment Record</div>
            <div class="line-3"></div>
            <div class="line-4"></div>
            <div class="line-5"></div>
            <div class="text-wrapper-16">Patient List</div>
            <div class="text-wrapper-17">Patient Detail</div>
            <img class="icon-5" src="/img/icon-solid-right-arrow.svg" />
            <div class="group-3"></div>
            <div class="frame-wrapper">
                <div class="frame">
                    <img class="icon-6" src="/img/icon-add.svg" />
